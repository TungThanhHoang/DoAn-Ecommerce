{
  "kind": "collectionType",
  "collectionName": "bills",
  "info": {
    "name": "Bills",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "address": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "unconfirmed",
        "confirmed",
        "deliveried"
      ],
      "default": "unconfirmed",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "qrcode": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "id_code": {
      "type": "text"
    },
    "price": {
      "type": "text"
    },
    "cart": {
      "collection": "items"
    },
    "phone": {
      "type": "string"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "bills"
    },
    "payment": {
      "type": "string"
    }
  }
}
