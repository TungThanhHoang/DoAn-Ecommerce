{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "name": "Category",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "products": {
      "via": "category",
      "collection": "products"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    }
  }
}
